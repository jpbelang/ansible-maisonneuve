;
; BIND data file for local loopback interface
;
$TTL    86400
@       IN      SOA     ns.lab.mondomaine.interne. root.lab.mondomaine.interne. (
                     2021030102         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      ns.lab.mondomaine.interne.
ns      IN      A       192.168.86.244

;also list other computers
www     IN      A       192.168.86.128
